<% @nearby_users.each do |user| %>
  <% if current_user != user%>
    <div class="card-product">
      <div class="card-product-infos">
        <%= link_to user_conversations_path(user), method: :post do %>
          <%= user.username %>
        <% end %>
      </div>
    </div>
  <% end %>
<% end %>
