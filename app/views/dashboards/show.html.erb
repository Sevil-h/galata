      <%= link_to new_article_path, class: 'btn-dashboard m-2 justify-content-center align-items-center' do  %>
        <h1><i class="fas fa-book-open"></i></h1>
      <% end %>
      <%= link_to new_photo_path, class: 'btn-dashboard m-2' do %>
